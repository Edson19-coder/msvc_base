CREATE OR REPLACE PACKAGE MSVC_BASE.PKG_COURSE AS TYPE CURSORTYPE IS REF CURSOR;
  PROCEDURE CREATE_COURSE(vCourseName IN VARCHAR2, vResponse OUT VARCHAR2);
  PROCEDURE UPDATE_COURSE(vCourseId IN NUMBER, vCourseName IN VARCHAR2, vStatus IN VARCHAR2, vResponse OUT VARCHAR2);
  PROCEDURE DELETE_COURSE(vCourseId IN NUMBER, vResponse OUT VARCHAR2);
  PROCEDURE GET_COURSE(vCourseId IN VARCHAR2, rc OUT SYS_REFCURSOR);
  PROCEDURE GET_COURSES(rc OUT SYS_REFCURSOR);
END PKG_COURSE;