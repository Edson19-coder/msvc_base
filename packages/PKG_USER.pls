CREATE OR REPLACE PACKAGE MSVC_BASE.PKG_USER AS TYPE CURSORTYPE IS REF CURSOR;
  PROCEDURE CREATE_USER(vUserName IN VARCHAR2, vEmail IN VARCHAR2, vPassword IN VARCHAR, vResponse OUT VARCHAR2);
  PROCEDURE UPDATE_USER(vUserId IN NUMBER, vUserName IN VARCHAR2, vEmail IN VARCHAR2, vPassword IN VARCHAR, vActive IN NUMBER, vResponse OUT VARCHAR2);
  PROCEDURE GET_USER(vEmail IN VARCHAR2, rc OUT SYS_REFCURSOR);
  PROCEDURE GET_USERS(rc OUT SYS_REFCURSOR);
END PKG_USER;